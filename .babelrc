{
    "plugins": [
        ["module-resolver", 
            {
                "root": ["."],
                "extensions": [".ts",".tsx",".jsx",".js",".json",".svg",".jpg"],
                "alias": {
                    "^@myapp/(.+)": "./src/\\1",
                    "^@myassets/(.+)": "./assets/\\1"
                }
            }
        ]
    ]
}